\n\r\nconst SigninComponent = () => {\r\n    const [values, setValues] = useState({\r\n        email: '',\r\n        password: '',\r\n        error: '',\r\n        success: '',\r\n        message: '',\r\n        loading: ''\r\n    })\r\n    useEffect(()=>{\r\n        isAuth() && Router.push('/')\r\n    },[])\r\n    const { email, password, error, success, message, loading} = values\r\n    const handleChange = name => event => {\r\n        setValues({...values,error:'',[name]:event.target.value})\r\n    }\r\n    const handleSubmit = (event) => {\r\n        event.preventDefault()\r\n        setValues({...values, loading: true})\r\n        signin({ email, password}).then(data => {\r\n            if(data.error){\r\n                setValues({...values, error:data.error,success: false, loading: false})\r\n            }else{\r\n                setValues({\r\n                    ...values,\r\n                    error:'',\r\n                    loading:false,\r\n                    success: true,\r\n                    message: data.message\r\n                })\r\n                authenticate(data,() => {\r\n                    Router.push('/')\r\n                })\r\n                \r\n            }\r\n\r\n        })\r\n    }\r\n    const showLoading = () => {\r\n        return (\r\n            loading?<div className=\"alert alert-info\">Loading...</div>:\"\"\r\n        )\r\n    }\r\n\r\n    const showError = () => {\r\n        return (\r\n            error?<div className=\"alert alert-danger\">{error}</div>:\"\"\r\n        )\r\n    }\r\n\r\n    const showMessage = () => {\r\n        return (\r\n            message?<div className=\"alert alert-info\">{message}</div>:\"\"\r\n        )\r\n    }\r\n    const togglepassword = () => {\r\n        if(document.getElementById(\"mypass\").type==\"text\"){\r\n            document.getElementById(\"mypass\").type = \"password\"\r\n        }else{\r\n            document.getElementById(\"mypass\").type = \"text\"\r\n        }\r\n    }\r\n    const signinForm = () => {\r\n        return(\r\n            <div className=\"container col-md-8 mt-5\" style = {{margin:'auto'}}>\r\n                \r\n            <div className=\"row justify-content-center mb-5\">\r\n                    <h2>Signup</h2>\r\n                </div>\r\n                {showError()}\r\n                {showMessage()}\r\n                \r\n            <form onSubmit={handleSubmit}>\r\n                <fieldset className=\"form-group\">\r\n                    <label className=\"text-muted\">Email</label>\r\n                    <input value= {email} onChange={handleChange('email')} type=\"email\" id=\"myemail\" className=\"form-control\" required/>\r\n                    <span className=\"bmd-help\">We'll never share your email with anyone else.</span>\r\n                </fieldset>\r\n\r\n                <div className=\"form-group\">\r\n                    <label className=\"text-muted\">Password</label>\r\n                    <input value={password} onChange={handleChange('password')} id=\"mypass\" type=\"password\"  className=\"form-control\" required minLength=\"8\"></input>\r\n                    <input type=\"checkbox\" onClick={()=>togglepassword()} /><span> show password</span>\r\n                </div>\r\n                <button type=\"submit\" className=\"btn btn-primary\">Submit</button>\r\n            </form>\r\n            <div>\r\n                <p className=\"info\">Not Registerd <Link href=\"/signup\">signup</Link></p>\r\n            </div>\r\n            </div>\r\n        )\r\n    }\r\n    return (\r\n        <React.Fragment>\r\n        {signinForm()}\r\n        {showLoading()}\r\n        </React.Fragment>\r\n    )\r\n}\r\nexport default SigninComponent;"]},"metadata":{},"sourceType":"module"}