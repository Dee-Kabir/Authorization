{"ast":null,"code":"import { API } from \"../config\";\nimport cookie from \"js-cookie\"; // signup\n\nexport var signup = function signup(user) {\n  return fetch(\"\".concat(API, \"/signup\"), {\n    method: \"POST\",\n    headers: {\n      Accept: \"application/json\",\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(user)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (error) {\n    return console.log(error);\n  });\n}; // signin\n\nexport var signin = function signin(user) {\n  return fetch(\"\".concat(API, \"/signin\"), {\n    method: \"POST\",\n    headers: {\n      Accept: \"application/json\",\n      \"Content-Type\": \"application/json\"\n    },\n    body: JSON.stringify(user)\n  }).then(function (response) {\n    return response.json();\n  })[\"catch\"](function (error) {\n    return console.log(error);\n  });\n}; // signout\n\nexport var signout = function signout(next) {\n  removeCookie(\"token\");\n  removeLocalStorage(\"user\");\n  return fetch(\"\".concat(API, \"/signout\"), {\n    method: \"GET\"\n  }).then(function (response) {\n    next();\n    return response.json();\n  })[\"catch\"](function (error) {\n    return console.log(error);\n  });\n}; // set cookie\n\nexport var setCookie = function setCookie(key, value) {\n  if (true) {\n    cookie.set(key, value, {\n      expires: 1\n    });\n  }\n}; // remove cookie\n\nexport var removeCookie = function removeCookie(key) {\n  if (true) {\n    cookie.remove(key);\n  }\n}; // get cookie\n\nexport var getCookie = function getCookie(key) {\n  if (true) {\n    return cookie.get(key);\n  }\n}; // localstorage set, remove, get\n\nexport var setLocalStorage = function setLocalStorage(key, value) {\n  if (true) {\n    localStorage.setItem(key, JSON.stringify(value));\n  }\n};\nexport var removeLocalStorage = function removeLocalStorage(key) {\n  if (true) {\n    localStorage.removeItem(key);\n  }\n}; // authenticate user on signin\n\nexport var authenticate = function authenticate(data, next) {\n  setCookie(\"token\", data.token);\n  setLocalStorage(\"user\", data.user);\n  next();\n}; // check authenticated or not\n\nexport var isAuth = function isAuth() {\n  if (true) {\n    if (getCookie(\"token\")) {\n      if (localStorage.getItem(\"user\")) {\n        return JSON.parse(localStorage.getItem(\"user\"));\n      }\n\n      return false;\n    }\n\n    return false;\n  }\n\n  return false;\n};","map":{"version":3,"sources":["D:/authenticattion/frontend/actions/auth.js"],"names":["API","cookie","signup","user","fetch","method","headers","Accept","body","JSON","stringify","then","response","json","error","console","log","signin","signout","next","removeCookie","removeLocalStorage","setCookie","key","value","set","expires","remove","getCookie","get","setLocalStorage","localStorage","setItem","removeItem","authenticate","data","token","isAuth","getItem","parse"],"mappings":"AAAA,SAASA,GAAT,QAAoB,WAApB;AACA,OAAOC,MAAP,MAAmB,WAAnB,C,CAEA;;AACA,OAAO,IAAMC,MAAM,GAAG,SAATA,MAAS,CAACC,IAAD,EAAU;AAC9B,SAAOC,KAAK,WAAIJ,GAAJ,cAAkB;AAC5BK,IAAAA,MAAM,EAAE,MADoB;AAE5BC,IAAAA,OAAO,EAAE;AACPC,MAAAA,MAAM,EAAE,kBADD;AAEP,sBAAgB;AAFT,KAFmB;AAM5BC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,IAAf;AANsB,GAAlB,CAAL,CAQJQ,IARI,CAQC,UAACC,QAAD,EAAc;AAClB,WAAOA,QAAQ,CAACC,IAAT,EAAP;AACD,GAVI,WAWE,UAACC,KAAD;AAAA,WAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAAX;AAAA,GAXF,CAAP;AAYD,CAbM,C,CAeP;;AACA,OAAO,IAAMG,MAAM,GAAG,SAATA,MAAS,CAACd,IAAD,EAAU;AAC9B,SAAOC,KAAK,WAAIJ,GAAJ,cAAkB;AAC5BK,IAAAA,MAAM,EAAE,MADoB;AAE5BC,IAAAA,OAAO,EAAE;AACPC,MAAAA,MAAM,EAAE,kBADD;AAEP,sBAAgB;AAFT,KAFmB;AAM5BC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,IAAf;AANsB,GAAlB,CAAL,CAQJQ,IARI,CAQC,UAACC,QAAD,EAAc;AAClB,WAAOA,QAAQ,CAACC,IAAT,EAAP;AACD,GAVI,WAWE,UAACC,KAAD;AAAA,WAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAAX;AAAA,GAXF,CAAP;AAYD,CAbM,C,CAeP;;AACA,OAAO,IAAMI,OAAO,GAAG,SAAVA,OAAU,CAACC,IAAD,EAAU;AAC/BC,EAAAA,YAAY,CAAC,OAAD,CAAZ;AACAC,EAAAA,kBAAkB,CAAC,MAAD,CAAlB;AACA,SAAOjB,KAA