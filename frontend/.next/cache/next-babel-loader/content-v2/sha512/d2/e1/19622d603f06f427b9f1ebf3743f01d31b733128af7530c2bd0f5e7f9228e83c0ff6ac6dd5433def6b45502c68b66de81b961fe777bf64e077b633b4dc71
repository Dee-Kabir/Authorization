)) {\n        return JSON.parse(localStorage.getItem(\"user\"));\n      }\n\n      return false;\n    }\n\n    return false;\n  }\n\n  return false;\n};","map":{"version":3,"sources":["D:/authenticattion/frontend/actions/auth.js"],"names":["API","cookie","signup","user","fetch","method","headers","Accept","body","JSON","stringify","then","response","json","catch","error","console","log","signin","signout","next","removeCookie","removeLocalStorage","setCookie","key","value","set","expires","remove","getCookie","get","setLocalStorage","localStorage","setItem","removeItem","authenticate","data","token","isAuth","getItem","parse"],"mappings":"AAAA,SAASA,GAAT,QAAoB,WAApB;AACA,OAAOC,MAAP,MAAmB,WAAnB,C,CAEA;;AACA,OAAO,MAAMC,MAAM,GAAIC,IAAD,IAAU;AAC9B,SAAOC,KAAK,CAAE,GAAEJ,GAAI,SAAR,EAAkB;AAC5BK,IAAAA,MAAM,EAAE,MADoB;AAE5BC,IAAAA,OAAO,EAAE;AACPC,MAAAA,MAAM,EAAE,kBADD;AAEP,sBAAgB;AAFT,KAFmB;AAM5BC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,IAAf;AANsB,GAAlB,CAAL,CAQJQ,IARI,CAQEC,QAAD,IAAc;AAClB,WAAOA,QAAQ,CAACC,IAAT,EAAP;AACD,GAVI,EAWJC,KAXI,CAWGC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAXb,CAAP;AAYD,CAbM,C,CAeP;;AACA,OAAO,MAAMG,MAAM,GAAIf,IAAD,IAAU;AAC9B,SAAOC,KAAK,CAAE,GAAEJ,GAAI,SAAR,EAAkB;AAC5BK,IAAAA,MAAM,EAAE,MADoB;AAE5BC,IAAAA,OAAO,EAAE;AACPC,MAAAA,MAAM,EAAE,kBADD;AAEP,sBAAgB;AAFT,KAFmB;AAM5BC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,IAAf;AANsB,GAAlB,CAAL,CAQJQ,IARI,CAQEC,QAAD,IAAc;AAClB,WAAOA,QAAQ,CAACC,IAAT,EAAP;AACD,GAVI,EAWJC,KAXI,CAWGC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAXb,CAAP;AAYD,CAbM,C,CAeP;;AACA,OAAO,MAAMI,OAAO,GAAIC,IAAD,IAAU;AAC/BC,EAAAA,YAAY,CAAC,OAAD,CAAZ;AACAC,EAAAA,kBAAkB,CAAC,MAAD,CAAlB;AACA,SAAOlB,KAAK,CAAE,GAAEJ,GAAI,UAAR,EAAmB;AAC7BK,IAAAA,MAAM,EAAE;AADqB,GAAnB,CAAL,CAGJM,IAHI,CAGEC,QAAD,IAAc;AAClBQ,IAAAA,IAAI;AACJ,WAAOR,QAAQ,CAACC,IAAT,EAAP;AACD,GANI,EAOJC,KAPI,CAOGC,KAAD,IAAWC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAPb,CAAP;AAQD,CAXM,C,CAaP;;AACA,OAAO,MAAMQ,SAAS,GAAG,CAACC,GAAD,EAAMC,KAAN,KAAgB;AACvC,aAAqB;AACnBxB,IAAAA,MAAM,CAACyB,GAAP,CAAWF,GAAX,EAAgBC,KAAhB