ion authenticate(data, next) {\n  setCookie('token', data.token);\n  setLocalStorage('user', data.user);\n  next();\n}; // check authenticated or not\n\nexport var isAuth = function isAuth() {\n  if (true) {\n    if (getCookie('token')) {\n      if (localStorage.getItem('user')) {\n        return JSON.parse(localStorage.getItem('user'));\n      }\n\n      return false;\n    }\n\n    return false;\n  }\n\n  return false;\n};","map":{"version":3,"sources":["D:/authenticattion/frontend/actions/auth.js"],"names":["API","cookie","signup","user","fetch","method","headers","Accept","body","JSON","stringify","then","response","json","error","console","log","signin","signout","next","removeCookie","removeLocalStorage","setCookie","key","value","set","expires","remove","getCookie","get","setLocalStorage","localStorage","setItem","removeItem","authenticate","data","token","isAuth","getItem","parse"],"mappings":"AAAA,SAAQA,GAAR,QAAkB,WAAlB;AACA,OAAOC,MAAP,MAAmB,WAAnB,C,CAEA;;AACA,OAAO,IAAMC,MAAM,GAAG,SAATA,MAAS,CAACC,IAAD,EAAU;AAC5B,SAAOC,KAAK,WAAIJ,GAAJ,cAAiB;AACzBK,IAAAA,MAAM,EAAC,MADkB;AAEzBC,IAAAA,OAAO,EAAC;AACJC,MAAAA,MAAM,EAAC,kBADH;AAEJ,sBAAgB;AAFZ,KAFiB;AAMzBC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,IAAf;AANmB,GAAjB,CAAL,CAOJQ,IAPI,CAOC,UAAAC,QAAQ,EAAI;AAChB,WAAOA,QAAQ,CAACC,IAAT,EAAP;AACH,GATM,WASE,UAAAC,KAAK;AAAA,WAAIC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAAJ;AAAA,GATP,CAAP;AAUH,CAXM,C,CAcP;;AACA,OAAO,IAAMG,MAAM,GAAG,SAATA,MAAS,CAACd,IAAD,EAAU;AAC5B,SAAOC,KAAK,WAAIJ,GAAJ,cAAiB;AACzBK,IAAAA,MAAM,EAAC,MADkB;AAEzBC,IAAAA,OAAO,EAAC;AACJC,MAAAA,MAAM,EAAC,kBADH;AAEJ,sBAAgB;AAFZ,KAFiB;AAMzBC,IAAAA,IAAI,EAAEC,IAAI,CAACC,SAAL,CAAeP,IAAf;AANmB,GAAjB,CAAL,CAOJQ,IAPI,CAOC,UAAAC,QAAQ,EAAI;AAChB,WAAOA,QAAQ,CAACC,IAAT,EAAP;AACH,GATM,WASE,UAAAC,KAAK;AAAA,WAAIC,OAAO,CAACC,GAAR,CAAYF,KAAZ,CAAJ;AAAA,GATP,CAAP;AAUH,CAXM,C,CAaP;;AACA,OAAO,IAAMI,OAAO,GAAG,SAAVA,OAAU,CAACC,IAAD,EAAU;AAC7BC,EAAAA,YAAY,CAAC,OAAD,CAAZ;AACAC,EAAAA,kBAAkB,CAAC,MAAD,CAAlB;AACA,SAAOjB,KAAK,WAAIJ,GAAJ,eAAkB;AAC1BK,IAAAA,MAAM